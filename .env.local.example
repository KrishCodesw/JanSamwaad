# Copy this file to .env.local and fill values from your Supabase project settings
# Dashboard → Settings → API

# Public URL of your Supabase instance
NEXT_PUBLIC_SUPABASE_URL="https://your-project-ref.supabase.co"

# Public anon (publishable) key
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-publishable-key"

# Some server code uses a different env name; set both to be safe
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_OR_ANON_KEY="your-anon-publishable-key"

# Service role (ADMIN) key — keep secret, do NOT commit
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# Optional: NEXTAUTH or other keys if used by your project
# NEXTAUTH_SECRET=...
# OTHER_SECRETS=...
